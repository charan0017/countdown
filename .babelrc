{
    "presets": [
        [
        	"@babel/preset-env",
        	{
        		"targets": {
        			"browsers": [ "last 1 version", "ie >= 11" ]
      			},
                "exclude": ["transform-regenerator"]
            }
    	],
        "minify"
    ],
    "plugins": [
    	"babel-plugin-transform-async-to-promises",
    	"@babel/plugin-proposal-class-properties"
	]
}